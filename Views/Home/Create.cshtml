@model PersonalAccount

@{
    ViewBag.Title = "title";
    Layout = "_layout";
}

<h2>Заполните форму для добавления лицевого счета</h2>
<div class="row">
    <div class="toDo_add_form col-md-6">
        <form asp-action="Create" asp-controller="Home" method="post">
            <div asp-validation-summary="ModelOnly"></div>
            <div class="form_row">
                <label for="name">
                    Номер ЛС
                    <input id="name" type="text" asp-for="NumberPA"/>
                </label>
                <label for="name">
                    Дата начала действия
                    <input id="name" type="datetime-local" asp-for="StartDate" />
                </label>
                <label for="name">
                    Дата окончания  действия
                    <input id="name" type="datetime-local" asp-for="EndDate" />
                </label>
                <label for="address">
                    Адрес
                    <input id="name" type="text" asp-for="Address">
                </label>
                <label for="roomArea">
                    Площадь помещения
                    <input id="roomArea" type="text" asp-for="RoomArea"/>
                </label>
                <button class="btn btn-outline-warning">Создать</button>
            </div>
        </form>
    </div>
